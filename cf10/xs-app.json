{
    "welcomeFile": "index.html",
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/backend(.*)",
            "target": "$1",
            "destination": "demo21dest",
            "authenticationType": "BasicAuthentication"
        },
        {
            "source": "^(.*)$",
            "target": "$1",
            "authenticationType": "none",
            "localDir": "./"
        }
    ]
}